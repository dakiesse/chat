{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended"
  ],
  "jsRules": {},
  "rules": {
    "semicolon": [
      true,
      "never"
    ],
    "quotemark": [
      true,
      "single"
    ],
    "no-console": false,
    "space-before-function-paren": [
      true,
      "always"
    ],
    "trailing-comma": [
      true,
      {
        "multiline": "always",
        "singleline": "never"
      }
    ],
    "object-literal-sort-keys": false,
    "ordered-imports": false
  },
  "rulesDirectory": []
}
