{
  "name": "steamcasino",
  "version": "1.0.0",
  "license": "MIT",
  "main": "./backend/server.ts",
  "scripts": {
    "start": "nodemon -w backend -x ts-node backend/app.ts --no-cache",
    "start+": "tsc -w && NODE_PATH=./build nodemon ./build/app.js",
    "build": "tsc",
    "start++": "./node_modules/nodemon/bin/nodemon.js -e ts  --exec \"npm run compile\"",
    "compile": "tsc && NODE_PATH=./build node ./build/app.js",
    "clear": "find ./backend -name \"*.js\" -type f -delete",
    "lint": "tslint backend/**/*.ts --force --format stylish"
  },
  "dependencies": {
    "chalk": "^1.1.3",
    "dotenv": "^4.0.0",
    "koa": "2.2.0",
    "koa-bodyparser": "^4.2.0",
    "koa-logger": "^2.0.1",
    "koa-passport": "^3.0.0",
    "koa-pug": "^3.0.0-2",
    "koa-redis": "^3.0.0",
    "koa-router": "next",
    "koa-session-minimal": "^3.0.3",
    "koa-static": "^3.0.0",
    "mongoose": "^4.9.4",
    "passport-facebook": "^2.1.1",
    "passport-google-oauth": "^1.0.0"
  },
  "devDependencies": {
    "@types/chalk": "^0.4.31",
    "@types/koa": "^2.0.39",
    "@types/koa-bodyparser": "^3.0.23",
    "@types/koa-logger": "^2.0.2",
    "@types/koa-passport": "^2.0.4",
    "@types/koa-pug": "^3.0.2",
    "@types/koa-router": "^7.0.22",
    "@types/koa-session-minimal": "^3.0.4",
    "@types/koa-static": "^2.0.22",
    "@types/mongoose": "^4.7.10",
    "@types/node": "^7.0.12",
    "@types/passport-facebook": "^2.1.4",
    "@types/passport-google-oauth": "^1.0.34",
    "@types/dotenv": "^4.0.0",
    "nodemon": "^1.11.0",
    "tslint": "^5.1.0",
    "typescript": "^2.3.0"
  }
}
