{
  "name": "chat",
  "version": "1.0.0",
  "license": "MIT",
  "main": "./backend/server.ts",
  "scripts": {
    "start": "nodemon -w backend -x ts-node src/backend/app.ts --no-cache",
    "start+": "tsc -w && NODE_PATH=./dist nodemon ./dist/app.js",
    "build": "tsc",
    "start++": "./node_modules/nodemon/bin/nodemon.js -e ts --exec \"npm run compile\"",
    "compile": "tsc --project ./src/backend && NODE_PATH=./dist node --inspect ./dist/app.js",
    "clear": "find ./backend -name \"*.js\" -type f -delete",
    "lint": "tslint backend/**/*.ts --force --format stylish"
  },
  "dependencies": {
    "@angular/common": "^4.0.0",
    "@angular/compiler": "^4.0.0",
    "@angular/core": "^4.0.0",
    "@angular/forms": "^4.0.0",
    "@angular/http": "^4.0.0",
    "@angular/platform-browser": "^4.0.0",
    "@angular/platform-browser-dynamic": "^4.0.0",
    "@angular/router": "^4.0.0",
    "@types/jsonwebtoken": "^7.2.0",
    "@types/koa-jwt": "^3.2.0",
    "@types/passport-jwt": "^2.0.19",
    "chalk": "^2.0.1",
    "core-js": "^2.4.1",
    "dotenv": "^4.0.0",
    "koa": "2.3.0",
    "koa-bodyparser": "^4.2.0",
    "koa-jwt": "^3.2.1",
    "koa-logger": "^3.0.1",
    "koa-passport": "^3.0.0",
    "koa-pug": "^3.0.0-2",
    "koa-redis": "^3.0.0",
    "koa-router": "next",
    "koa-session-minimal": "^3.0.3",
    "koa-static": "^4.0.0",
    "mongoose": "^4.11.1",
    "passport-facebook": "^2.1.1",
    "passport-google-oauth": "^1.0.0",
    "passport-jwt": "^2.2.1",
    "rxjs": "^5.1.0",
    "zone.js": "^0.8.4"
  },
  "devDependencies": {
    "@angular/cli": "1.2.0",
    "@angular/compiler-cli": "^4.0.0",
    "@types/chalk": "^0.4.31",
    "@types/dotenv": "^4.0.0",
    "@types/jasmine": "2.5.53",
    "@types/koa": "^2.0.39",
    "@types/koa-bodyparser": "^3.0.23",
    "@types/koa-logger": "^2.0.2",
    "@types/koa-passport": "^2.0.4",
