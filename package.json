    "@types/mongoose": "^4.7.18",
    "@types/node": "^8.0.7",
    "@types/passport-facebook": "^2.1.4",
    "@types/passport-google-oauth": "^1.0.34",
    "codelyzer": "~3.1.2",
    "jasmine-core": "~2.6.4",
    "jasmine-spec-reporter": "~4.1.1",
    "karma": "~1.7.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^1.3.0",
